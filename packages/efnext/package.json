{
  "name": "@typespec/efnext",
  "version": "0.56.0",
  "author": "Microsoft Corporation",
  "description": "emitter framework prototype",
  "homepage": "https://github.com/microsoft/typespec",
  "readme": "https://github.com/microsoft/typespec/blob/main/README.md",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microsoft/typespec.git"
  },
  "bugs": {
    "url": "https://github.com/microsoft/typespec/issues"
  },
  "keywords": [
    "TypeSpec",
    "json schema"
  ],
  "type": "module",
  "main": "dist/src/cli-sketch/src/index.js",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    },
    "./testing": {
      "types": "./dist/src/testing/index.d.ts",
      "default": "./dist/src/testing/index.js"
    }
  },
  "tspMain": "src/cli-sketch/lib/main.tsp",
  "engines": {
    "node": ">=18.0.0"
  },
  "imports": {
    "#jsx/jsx-runtime": {
      "default": "./dist/src/framework/core/jsx.js"
    },
    "#jsx/jsx-dev-runtime": {
      "default": "./dist/src/framework/core/jsx.js"
    },
    "#typespec/emitter/python": {
      "default": "./dist/src/python/index.js"
    },
    "#typespec/emitter/typescript": {
      "default": "./dist/src/typescript/index.js"
    },
    "#typespec/emitter/core": {
      "default": "./dist/src/framework/index.js"
    },
    "#typespec-cli": {
      "default": "./dist/src/cli-sketch/src/index.js"
    }
  },
  "scripts": {
    "clean": "rimraf ./dist ./temp",
    "build": "tsc -p .",
    "watch": "tsc -p . --watch",
    "gen-extern-signature": "tspd --enable-experimental gen-extern-signature .",
    "lint-typespec-library": "tsp compile . --warn-as-error --import @typespec/library-linter --no-emit",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:ci": "vitest run --coverage --reporter=junit --reporter=default",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "regen-docs": "tspd doc .  --enable-experimental  --output-dir ../../docs/emitters/json-schema/reference"
  },
  "files": [
    "lib/*.tsp",
    "dist/**",
    "!dist/test/**"
  ],
  "peerDependencies": {
    "@typespec/compiler": "workspace:~",
    "@typespec/http": "workspace:~"
  },
  "devDependencies": {
    "@types/node": "~18.11.19",
    "@typespec/compiler": "workspace:~",
    "@typespec/internal-build-utils": "workspace:~",
    "@typespec/library-linter": "workspace:~",
    "@typespec/tspd": "workspace:~",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "ajv": "~8.13.0",
    "ajv-formats": "~3.0.1",
    "c8": "^9.1.0",
    "rimraf": "~5.0.7",
    "typescript": "~5.4.5",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@types/marked-terminal": "^6.1.1",
    "change-case": "~5.4.4",
    "cli-table3": "^0.6.5",
    "marked": "^12.0.2",
    "marked-terminal": "^7.0.0",
    "picocolors": "~1.0.1",
    "prettier": "~3.2.5",
    "strip-ansi": "^7.1.0",
    "yaml": "~2.4.2"
  }
}
